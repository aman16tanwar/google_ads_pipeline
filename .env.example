GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json
GOOGLE_ADS_JSON_KEY_FILE_PATH=/path/to/service-account.json

# BigQuery Configuration
BIGQUERY_DATASET_ALL_MAIN=your_dataset_name
